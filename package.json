{
  "name": "job-checker",
  "version": "2.2.1",
  "private": true,
  "packageManager": "pnpm@8.3.1",
  "description": "Checks which jobs are ",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Rotki Solutions GmbH",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "preinstall": "npx only-allow pnpm",
    "test": "jest",
    "all": "pnpm run build && pnpm run lint && pnpm run package && pnpm run test",
    "prepare": "husky install",
    "release": "bumpp --no-push"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@rotki/eslint-config": "1.1.1",
    "@types/jest": "29.5.4",
    "@types/node": "20.9.2",
    "@vercel/ncc": "0.36.1",
    "bumpp": "9.2.0",
    "eslint": "8.48.0",
    "eslint-plugin-github": "4.9.2",
    "eslint-plugin-jest": "27.2.3",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-circus": "29.6.4",
    "js-yaml": "4.1.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.2",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=8 <9"
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint"
  }
}
