{
  "name": "job-checker",
  "version": "2.2.1",
  "private": true,
  "packageManager": "pnpm@8.3.1",
  "description": "Checks which jobs are ",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Rotki Solutions GmbH",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "preinstall": "npx only-allow pnpm",
    "test": "jest",
    "all": "pnpm run build && pnpm run lint && pnpm run package && pnpm run test",
    "prepare": "husky install",
    "release": "bumpp --no-push"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@rotki/eslint-config": "1.1.2",
    "@types/jest": "29.5.8",
    "@types/node": "20.9.2",
    "@vercel/ncc": "0.38.1",
    "bumpp": "9.2.0",
    "eslint": "8.54.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-jest": "27.6.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "js-yaml": "4.1.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=8 <9"
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint"
  }
}
