{
    "plugins": ["jest", "@typescript-eslint"],
    "extends": ["plugin:github/recommended", "@rotki"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "project": "./tsconfig.eslint.json"
    },
    "rules": {
      "import/no-namespace": "off",
      "i18n-text/no-en": "off"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest/globals": true
    }
  }